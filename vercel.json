{
  "version": 2,
  "builds": [
    {
      "src": "site/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/wasm/(.*)",
      "headers": {
        "Content-Type": "application/wasm",
        "Cross-Origin-Embedder-Policy": "require-corp",
        "Cross-Origin-Opener-Policy": "same-origin"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/site/$1"
    }
  ],
  "headers": [
    {
      "source": "/wasm/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}

